@inject SignInManager<AppUser> SignInManager

<link rel="stylesheet" href="~/css/resume.css" asp-append-version="true">
<script src="~/js/resume.js" asp-append-version="true"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">

<body>
    <div class="section">
        <h1 class="text-center name">Yunho Nam</h1>
        <h6 class="text-center subname">Also David Nam</h6>
    </div>
    @*Objective*@
    <div class="section objective" id="objective">
        <h3 class="text-center">Objective</h3>
        <div id="objective-edit-load1">
            <i class="bi bi-pencil-square objective-edit-btn" onclick="objectiveEditor1()"></i>
            <p class="text-center objective-p">
                This is my objective.This is my objective.This is my objective.This is my objective.This is my objective.
                This is my objective.This is my objective.This is my objective.This is my objective.This is my objective.
                This is my objective.This is my objective.This is my objective.This is my objective.This is my objective.
            </p>
        </div>
        <div id="objective-edit-load2" hidden="hidden">
            <form novalidate id="objective-edit-form">
                <textarea class="objective-textarea"></textarea>
                <div class="objective-btns">
                    <input class="btn btn-primary save-btn" value="Save" onclick="">
                    <input class="btn btn-danger cancel-btn ms-3" value="Cancel" onclick="objectiveEditor2()">
                </div>
            </form>
        </div>
    </div>
    @*Education*@
    <div class="section education">
        <h3 class="text-center">Education</h3>
    </div>
    @*Skills*@
    <div class="section skills">
        <h3 class="text-center">Skills</h3>
    </div>
    @*Work Experience*@
    <div class="section experience">
        <h3 class="text-center">Work Experience</h3>
    </div>
</body>
